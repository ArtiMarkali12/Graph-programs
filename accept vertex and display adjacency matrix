#include<stdio.h>
void main()
{
	int a[10][10];
	int i,n,j,sumin=0,sumout=0;
	printf("enter the number of vertex:");
	scanf("%d",&n);
	printf("enter 1 if edge is present otherwise 0:");
	for(i=0;i<n;i++)
	{
	for(j=0;j<n;j++)
	{
	a[i][j]=0;
	if(i!=j)
	{
	printf("enter value:");
	scanf("%d",&a[i][j]);
	}
}
}
	printf("\n adjacency matrix:\n");
	for(i=0;i<n;i++)
	{
	printf("\n");
	for(j=0;j<n;j++)
	
	printf("%d\t",a[i][j]);
	}
	printf("\n");
	}
